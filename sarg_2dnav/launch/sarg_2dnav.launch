<launch>
  <master auto="start"/>

  <!-- quori_description -->
  <param name="robot_description" command="cat $(find quori_description)/urdf/quori.xacro"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

  <!-- load map -->
  <node name="map" pkg="map_server" type="map_server" args="$(find sarg_2dnav)/maps/lab.yaml"/>

  <!-- load move_base and configuration -->
  <include file="$(find quori_nav)/launch/move_base.launch"/>
</launch>
